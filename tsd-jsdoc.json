{
  "source": {
    "include": [
      "src/browser/core.js",
      "src/server/rollbar.js",
      "src/browser/wrapGlobals.js",
      "src/telemetry.js",
      "src/tracing/tracing.js",
      "src/browser/replay/replay.js",
      "src/scrub.js",
      "src/truncation.js"
    ],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)node_modules"
  },
  "plugins": ["./node_modules/tsd-jsdoc/dist/plugin"],
  "opts": {
    "template": "./node_modules/tsd-jsdoc/dist",
    "destination": "tmp",
    "tsdModule": "rollbar",
    "tsdVersion": "3.0.0-rc.1",
    "outFile": "tsd-jsdoc.d.ts"
  }
}

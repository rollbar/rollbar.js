      "scrubFields": [
        "pw",
        "pass",
        "passwd",
        "password",
        "password_confirmation",
        "passwordConfirmation",
        "confirm_password",
        "confirmPassword",
        "secret",
        "secret_token",
        "secretToken",
        "secret_key",
        "secretKey",
        "api_key",
        "access_token",
        "accessToken",
        "authenticity_token",
        "oauth_token",
        "token",
        "user_session_secret",

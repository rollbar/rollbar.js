{
  "parserOpts": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "auto",
        "targets": {
          "ie": "11"
        }
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "allowDeclareFields": true,
        "onlyRemoveTypeImports": true
      }
    ]
  ]
}
